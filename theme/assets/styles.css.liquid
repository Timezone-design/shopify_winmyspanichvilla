  border-radius: {{ global_border_radius }}px;
}

.giftcard-qr img {
  display: block;
  margin: 0 auto;
}

.giftcard-apple-wallet {
  margin: 30px 0;
}

.apple-wallet-image {
  display: block;
  margin: 0 auto;
}

.giftcard-button-row {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 30px;
}

