.giftcard {
  margin: 30px auto;
  max-width: 500px;
  padding: 20px;
}

.giftcard__illustration {
  position: relative;
  margin: 20px 0;
  overflow: hidden;
  border-radius: {{ global_border_radius }}px;
}

.giftcard__illustration img {
  display: block;
}

.giftcard__code {
  background: #fff;
  border-radius: {{ global_border_radius }}px;
  color: #484848;
  position: absolute;
  bottom: 10%;
  right: 50%;
  transform: translateX(50%);
  padding: 10px 20px;
  font-size: 22px;
  line-height: 1em;
  white-space: nowrap;
}

.giftcard-qr-row {
  margin: 30px 0;
}

.giftcard-qr {
  display: inline-block;
